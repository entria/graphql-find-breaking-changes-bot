{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "no-restricted-syntax": 0,
    "max-len": [1, 120, 2],
    "no-param-reassign": [2, { "props": false }],
    "no-continue": 0,
    "no-underscore-dangle": 0,
    "generator-star-spacing": 0,
    "no-duplicate-imports": 0,
    "import/no-duplicates": 2,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "spaced-comment": 0,
    "prefer-const": [ 2, { "destructuring": "all" }],
    "guard-for-in": 0
  },
  "plugins": [
    "import"
  ],
  "env": {
    "es6": true,
    "node": true
  }
}
